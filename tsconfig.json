{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "@honkhonk/vite-plugin-svgr/client",
      "src/@types",
      "node_modules/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "@project/assets/*": ["src/assets/*"],
      "@project/containers": ["src/containers/index.ts"],
      "@project/components": ["src/components/index.ts"]
    }
  },
  "include": ["./src"]
}
